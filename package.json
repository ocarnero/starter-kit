    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
